        "max_pkg_size": 1000000,
        "heartbeat_check": false
    },
    "deals": {
        "brokers": "kafka:9092",
        "topic": "deals",
        "partition": 0
    },
    "redis": {
        "name": "mymaster",
        "addr": [
            "redis-sentinel-1:26381",
            "redis-sentinel-2:26382",
            "redis-sentinel-3:26383"
        ]
    },
    "accesshttp": "http://viabtc-ubuntu:8080"
